K,CAACM,MAAN,CAAa,oBAAb,EAAmC;AAAEP,YAAK,EAALA;AAAF,KAAnC;AACF,GAFA;;AAIA,SAAO;AAAEG,gBAAY,EAAZA,YAAF;AAAgBD,eAAW,EAAXA,WAAhB;AAA6BE,sBAAkB,EAAlBA,kBAA7B;AAAiDC,uBAAmB,EAAnBA,mBAAjD;AAAsEG,qBAAiB,EAAjBA,iBAAtE;AAAyFC,sBAAiB,EAAjBA;AAAzF,GAAP;AACF,CAjBA,C,CAmBA;;;AACA,IAAMC,gBAAe,GAAI,SAAnBA,gBAAmB,GAAM;AAC7B,MAAMC,QAAO,GAAIhB,GAAG,CAAC,KAAD,CAApB;;AACA,MAAMiB,oBAAmB,GAAI,SAAvBA,oBAAuB,GAAM;AACjCD,YAAQ,CAACE,KAAT,GAAiB,CAACF,QAAQ,CAACE,KAA3B;AACF,GAFA;;AAGA,SAAO;AAAEF,YAAQ,EAARA,QAAF;AAAYC,wBAAmB,EAAnBA;AAAZ,GAAP;AACF,CANA;;AAQA,eAAe;AACbE,MAAI,EAAE,MADO;AAEbC,OAFa,mBAEJ;AACP,QAAMC,KAAI,GAAIpB,QAAQ,EAAtB;AACA,QAAMI,MAAK,GAAIgB,KAAK,CAACC,MAAN,CAAaC,EAA5B;;AAEA,yBAAsHnB,aAAa,CAACC,MAAD,CAAnI;AAAA,QAAQG,YAAR,kBAAQA,YAAR;AAAA,QAAsBD,WAAtB,kBAAsBA,WAAtB;AAAA,QAAmCE,kBAAnC,kBAAmCA,kBAAnC;AAAA,QAAuDC,mBAAvD,kBAAuDA,mBAAvD;AAAA,QAA4EG,iBAA5E,kBAA4EA,iBAA5E;AAAA,QAA+FC,kBAA/F,kBAA+FA,kBAA/F;;AACA,4BAA2CC,gBAAgB,EAA3D;AAAA,QAAQC,QAAR,qBAAQA,QAAR;AAAA,QAAkBC,oBAAlB,qBAAkBA,oBAAlB;;AACA,WAAO;AAAET,kBAAY,EAAZA,YAAF;AAAgBH,YAAM,EAANA,MAAhB;AAAwBE,iBAAW,EAAXA,WAAxB;AAAqCS,cAAQ,EAARA,QAArC;AAA+CP,wBAAkB,EAAlBA,kBAA/C;AAAmEC,yBAAmB,EAAnBA,mBAAnE;AAAwFG,uBAAiB,EAAjBA,iBAAxF;AAA2GC,wBAAkB,EAAlBA,kBAA3G;AAA+HG,0BAAmB,EAAnBA;AAA/H,KAAP;AACF;AATa,CAAf","names":["ref","useRoute","useStore","useCommonCartEffect","useCartEffect","shopId","store","productList","calculations","changeCartItemInfo","changeCartItemCheck","productId","commit","cleanCartProducts","setCartItemChecked","toggleCartEffect","showCart","handleCartShowChange","value","name","setup","route","params","id"],"sourceRoot":"","sources":["C:\\Users\\a\\Desktop\\Vue3.0\\京东到家项目\\京东到家3\\jingdong\\src\\views\\shop\\Cart.vue"],"sourcesContent":["<template>\r\n<div\r\n class=\"mask\"\r\n v-if=\"showCart && calculations.total > 0\"\r\n @click=\"handleCartShowChange\"\r\n></div>\r\n<div class=\"cart\">\r\n  <div class=\"product\" v-if=\"showCart && calculations.total > 0\">\r\n    <div class=\"product__header\">\r\n      <div\r\n       class=\"product__header__all\"\r\n       @click=\"() => setCartItemChecked(shopId)\"\r\n      >\r\n        <span\r\n         class=\"product__header__icon iconfont\"\r\n         v-html=\"calculations.allChecked ? '&#xe6f7;' : '&#xe731;'\"\r\n        >\r\n        </span>\r\n        全选\r\n      </div>\r\n     <div class=\"product__header__clear\">\r\n      <span\r\n       class=\"product__header__clear__btn\"\r\n       @click=\"() => cleanCartProducts(shopId)\"\r\n      >清空购物车</span>\r\n     </div>\r\n    </div>\r\n    <div\r\n     v-for=\"item in productList\"\r\n     class=\"product__item\"\r\n     :key=\"item._id\"\r\n    >\r\n      <div\r\n       class=\"product__item__checked iconfont\"\r\n       v-html = \"item.check ? ' &#xe6f7; ' : '&#xe731;' \"\r\n       @click=\"() => changeCartItemCheck(shopId, item._id)\"\r\n      />\r\n      <img class=\"product__item__img\" :src=\"item.imgUrl\" />\r\n      <div class=\"product__item__detail\">\r\n        <h4 class=\"product__item__title\">{{item.name}}</h4>\r\n        <p class=\"product__item__price\">\r\n          <span class=\"product__item__yen\">&yen;</span>{{item.price}}\r\n          <span class=\"product__item__origin\">&yen;{{item.oldPrice}}</span>\r\n        </p>\r\n      </div>\r\n      <div class=\"product__number\">\r\n        <span\r\n         class=\"product__number__minus\"\r\n         @click=\"() => { changeCartItemInfo(shopId, item._id, item, -1) }\"\r\n        >-</span>\r\n          {{item.count || 0}}\r\n        <span\r\n          class=\"product__number__plus\"\r\n          @click=\"() => { changeCartItemInfo(shopId, item._id, item, 1) }\"\r\n        >+</span>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"check\">\r\n    <div class=\"check__icon\">\r\n      <img\r\n       src=\"http://www.dell-lee.com/imgs/vue3/basket.png\"\r\n       class=\"check__icon__img\"\r\n       @click=\"handleCartShowChange\"\r\n      >\r\n      <div class=\"check__icon__tag\">{{calculations.total}}</div>\r\n    </div>\r\n    <div class=\"check__info\">\r\n      总计 : <span class=\"check__info__price\">&yen; {{calculations.price}}</span>\r\n    </div>\r\n    <div class=\"check__btn\" v-show=\"calculations.total > 0\">\r\n      <router-link :to=\"{path: `/OrderConfirmation/${shopId}`}\">\r\n      去结算\r\n      </router-link>\r\n    </div>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue'\r\nimport { useRoute } from 'vue-router'\r\nimport { useStore } from 'vuex'\r\nimport { useCommonCartEffect } from '../../effects/cartEffects'\r\n\r\n// 获取购物车信息逻辑\r\nconst useCartEffect = (shopId) => {\r\n  const store = useStore()\r\n  const { productList, calculations, changeCartItemInfo } = useCommonCartEffect(shopId)\r\n\r\n  const changeCartItemCheck = (shopId, productId) => {\r\n    store.commit('changeCartItemChecked', { shopId, productId })\r\n  }\r\n\r\n  const cleanCartProducts = (shopId) => {\r\n    store.commit('cleanCartProducts', { shopId })\r\n  }\r\n\r\n  const setCartItemChecked = (shopId) => {\r\n    store.commit('setCartItemChecked', { shopId })\r\n  }\r\n\r\n  return { calculations, productList, changeCartItemInfo, changeCartItemCheck, cleanCartProducts, setCartItemChecked }\r\n}\r\n\r\n// 展示隐藏购物车逻辑\r\nconst toggleCartEffect = () => {\r\n  const showCart = ref(false)\r\n  const handleCartShowChange = () => {\r\n    showCart.value = !showCart.value\r\n  }\r\n  return { showCart, handleCartShowChange }\r\n}\r\n\r\nexport default {\r\n  name: 'Cart',\r\n  setup () {\r\n    const route = useRoute()\r\n    const shopId = route.params.id\r\n\r\n    const { calculations, productList, changeCartItemInfo, changeCartItemCheck, cleanCartProducts, setCartItemChecked } = useCartEffect(shopId)\r\n    const { showCart, handleCartShowChange } = toggleCartEffect()\r\n    return { calculations, shopId, productList, showCart, changeCartItemInfo, changeCartItemCheck, cleanCartProducts, setCartItemChecked, handleCartShowChange }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables';\r\n@import '../../style/mixins';\r\n.mask {\r\n  position: fixed;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  top: 0;\r\n  background: rgba(0, 0, 0, .5);\r\n  z-index: 1;\r\n}\r\n.cart {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  z-index: 2;\r\n  background: $bgColor;\r\n}\r\n.check {\r\n  display: flex;\r\n  height: .49rem;\r\n  border-top: .01rem solid $content-bgColor;\r\n  line-height: .49rem;\r\n  &__icon {\r\n    display: block;\r\n    position: relative;\r\n    width: .84rem;\r\n    &__img {\r\n      display: block;\r\n      width: .28rem;\r\n      height: .26rem;\r\n      margin: .12rem auto;\r\n    }\r\n    &__tag {\r\n      position: absolute;\r\n      left: .46rem;\r\n      top: .04rem;\r\n      padding: 0 .04rem;\r\n      min-width: .2rem;\r\n      height: .2rem;\r\n      line-height: .2rem;\r\n      background: $heightlight-fontColor;\r\n      border-radius: .1rem;\r\n      font-size: .12rem;\r\n      text-align: center;\r\n      color: $bgColor;\r\n      transform: scale(.5);\r\n      transform-origin: left center;\r\n    }\r\n  }\r\n  &__info {\r\n    flex: 1;\r\n    color: $content-fontcolor;\r\n    font-size: .12rem;\r\n    &__price {\r\n      color: $heightlight-fontColor;\r\n      font-size: .18rem;\r\n      margin-left: .05rem;\r\n    }\r\n  }\r\n  &__btn {\r\n    width: .98rem;\r\n    background: #4FB0F9;\r\n    text-align: center;\r\n    font-size: .14rem;\r\n    a {\r\n      color: $bgColor;\r\n      text-decoration: none;\r\n    }\r\n  }\r\n}\r\n.product {\r\n  overflow-y: scroll;\r\n  flex: 1;\r\n  background: $bgColor;\r\n  &__header {\r\n    display: flex;\r\n    line-height: .52rem;\r\n    border-bottom:1px solid $content-bgColor;\r\n    font-size: .14rem;\r\n    color: $content-fontcolor;\r\n    &__all {\r\n      width: .64rem;\r\n      margin-left: .18rem;\r\n    }\r\n    &__icon {\r\n      display: inline-block;\r\n      vertical-align: top;\r\n      margin-right: .1rem;\r\n      color: $btn-bgColor;\r\n      font-size: .2rem;\r\n    }\r\n    &__clear {\r\n      flex: 1;\r\n      margin-right: .16rem;\r\n      text-align: right;\r\n      &__btn {\r\n        display: inline-block;\r\n      }\r\n    }\r\n  }\r\n  &__item {\r\n    position: relative;\r\n    display: flex;\r\n    padding: .12rem 0;\r\n    margin: 0 .16rem;\r\n    border-bottom: .01rem solid $content-bgColor;\r\n    &__checked {\r\n      line-height: .5rem;\r\n      margin-right: .2rem;\r\n      color: $btn-bgColor;\r\n      font-size: .2rem;\r\n    }\r\n    &__img {\r\n      width: .46rem;\r\n      height: .46rem;\r\n    }\r\n    &__detail {\r\n      margin-left: .16rem;\r\n      overflow: hidden;\r\n    }\r\n    &__title {\r\n      margin: 0;\r\n      line-height: .2rem;\r\n      font-size: .12rem;\r\n      color: $content-fontcolor;\r\n      @include ellipsis;\r\n    }\r\n    &__price {\r\n      margin: .06rem 0 0 0;\r\n      font-size: .14rem;\r\n      color: #ac8c8c;\r\n      line-height: .2rem;\r\n    }\r\n    &__yen {\r\n      font-size: .12rem;\r\n    }\r\n    &__origin {\r\n      font-size: .12rem;\r\n      color: $light-fontColor;\r\n      line-height: 20px;\r\n      margin-left: .06rem;\r\n      text-decoration: line-through;\r\n    }\r\n    .product__number {\r\n      position: absolute;\r\n      bottom: .26rem;\r\n      right: 0;\r\n      &__minus, &__plus {\r\n        display: inline-block;\r\n        height: .2rem;\r\n        line-height: .16rem;\r\n        width: .2rem;\r\n        border-radius: 50%;\r\n        font-size: .2rem;\r\n        text-align: center;\r\n      }\r\n      &__minus {\r\n        border: 0.01rem solid $medium-fontColor;\r\n        color: $medium-fontColor;\r\n        margin-right: .05rem;\r\n      }\r\n      &__plus {\r\n        background: $btn-bgColor;\r\n        color: $bgColor;\r\n        margin-left: .05rem;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}