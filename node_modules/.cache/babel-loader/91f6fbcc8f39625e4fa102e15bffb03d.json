AAI,GAAI,CAAZ;;AACA,QAAID,WAAJ,EAAiB;AACf,WAAK,IAAME,CAAX,IAAgBF,WAAhB,EAA6B;AAC3B,YAAMG,OAAM,GAAIH,WAAW,CAACE,CAAD,CAA3B;;AACA,YAAIC,OAAO,CAACE,KAAZ,EAAmB;AACjBJ,eAAI,IAAME,OAAO,CAACF,KAAR,GAAgBE,OAAO,CAACC,KAAlC;AACF;AACF;AACF;;AACA,WAAOH,KAAK,CAACK,OAAN,CAAc,CAAd,CAAP;AACD,GAZqB,CAAtB;AAcA,MAAMN,WAAU,GAAIX,QAAQ,CAAC,YAAM;AACjC,QAAMW,WAAU,GAAIH,QAAQ,CAACH,MAAD,CAAR,IAAoB,EAAxC;AACA,WAAOM,WAAP;AACD,GAH2B,CAA5B;;AAKA,MAAMO,mBAAkB,GAAI,SAAtBA,mBAAsB,CAACb,MAAD,EAASc,SAAT,EAAuB;AACjDZ,SAAK,CAACa,MAAN,CAAa,uBAAb,EAAsC;AAAEf,YAAM,EAANA,MAAF;AAAUc,eAAQ,EAARA;AAAV,KAAtC;AACF,GAFA;;AAIA,SAAO;AAAET,SAAK,EAALA,KAAF;AAASK,SAAK,EAALA,KAAT;AAAgBP,YAAQ,EAARA,QAAhB;AAA0BG,eAAW,EAAXA,WAA1B;AAAuCL,kBAAc,EAAdA,cAAvC;AAAuDY,uBAAkB,EAAlBA;AAAvD,GAAP;AACF,CAvCA;;AAyCA,eAAe;AACbG,MAAI,EAAE,MADO;AAEbC,OAFa,mBAEJ;AACP,QAAMC,KAAI,GAAItB,QAAQ,EAAtB;AACA,QAAMI,MAAK,GAAIkB,KAAK,CAACC,MAAN,CAAaC,EAA5B;;AACA,yBAA2ErB,aAAa,CAACC,MAAD,CAAxF;AAAA,QAAQK,KAAR,kBAAQA,KAAR;AAAA,QAAeK,KAAf,kBAAeA,KAAf;AAAA,QAAsBJ,WAAtB,kBAAsBA,WAAtB;AAAA,QAAmCL,cAAnC,kBAAmCA,cAAnC;AAAA,QAAmDY,mBAAnD,kBAAmDA,mBAAnD;;AACA,WAAO;AAAER,WAAK,EAALA,KAAF;AAASK,WAAK,EAALA,KAAT;AAAgBV,YAAM,EAANA,MAAhB;AAAwBM,iBAAW,EAAXA,WAAxB;AAAqCL,oBAAc,EAAdA,cAArC;AAAqDY,yBAAkB,EAAlBA;AAArD,KAAP;AACF;AAPa,CAAf","names":["computed","useRoute","useStore","useCommonCartEffect","useCartEffect","shopId","changeItemInfo","store","cartList","state","total","productList","count","i","product","price","check","toFixed","changeCartItemCheck","productId","commit","name","setup","route","params","id"],"sourceRoot":"","sources":["C:\\Users\\a\\Desktop\\Vue3.0\\京东到家项目\\京东到家3\\jingdong\\src\\views\\shop\\Cart.vue"],"sourcesContent":["<template>\r\n<div class=\"cart\">\r\n  <div class=\"product\">\r\n    <div class=\"product__header\">\r\n\r\n    </div>\r\n    <template\r\n      v-for=\"item in productList\"\r\n      :key=\"item._id\"\r\n    >\r\n      <div class=\"product__item\" v-if=\"item.count > 0\">\r\n        <div\r\n         class=\"product__item__checked iconfont\"\r\n         v-html = \"item.check ? ' &#xe6f7; ' : '&#xe731;' \"\r\n         @click=\"() => changeCartItemCheck(shopId, item._id)\"\r\n        />\r\n        <img class=\"product__item__img\" :src=\"item.imgUrl\" />\r\n        <div class=\"product__item__detail\">\r\n          <h4 class=\"product__item__title\">{{item.name}}</h4>\r\n          <p class=\"product__item__price\">\r\n            <span class=\"product__item__yen\">&yen;</span>{{item.price}}\r\n            <span class=\"product__item__origin\">&yen;{{item.oldPrice}}</span>\r\n          </p>\r\n        </div>\r\n        <div class=\"product__number\">\r\n          <span\r\n           class=\"product__number__minus\"\r\n           @click=\"() => { changeItemInfo(shopId, item._id, item, -1) }\"\r\n          >-</span>\r\n            {{item.count || 0}}\r\n          <span\r\n            class=\"product__number__plus\"\r\n            @click=\"() => { changeItemInfo(shopId, item._id, item, 1) }\"\r\n          >+</span>\r\n        </div>\r\n      </div>\r\n    </template>\r\n  </div>\r\n  <div class=\"check\">\r\n    <div class=\"check__icon\">\r\n      <img\r\n       src=\"http://www.dell-lee.com/imgs/vue3/basket.png\"\r\n       class=\"check__icon__img\"\r\n      >\r\n      <div class=\"check__icon__tag\">{{total}}</div>\r\n    </div>\r\n    <div class=\"check__info\">\r\n      总计 : <span class=\"check__info__price\">&yen; {{price}}</span>\r\n    </div>\r\n    <div class=\"check__btn\">去结算</div>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nimport { useRoute } from 'vue-router'\r\nimport { useStore } from 'vuex'\r\nimport { useCommonCartEffect } from './commonCartEffect'\r\n\r\n// 获取购物车信息逻辑\r\nconst useCartEffect = (shopId) => {\r\n  const { changeItemInfo } = useCommonCartEffect()\r\n  const store = useStore()\r\n  const cartList = store.state.cartList\r\n  const total = computed(() => {\r\n    const productList = cartList[shopId]\r\n    let count = 0\r\n    if (productList) {\r\n      for (const i in productList) {\r\n        const product = productList[i]\r\n        count += product.count\r\n      }\r\n    }\r\n    return count\r\n  })\r\n  const price = computed(() => {\r\n    const productList = cartList[shopId]\r\n    let count = 0\r\n    if (productList) {\r\n      for (const i in productList) {\r\n        const product = productList[i]\r\n        if (product.check) {\r\n          count += (product.count * product.price)\r\n        }\r\n      }\r\n    }\r\n    return count.toFixed(2)\r\n  })\r\n\r\n  const productList = computed(() => {\r\n    const productList = cartList[shopId] || []\r\n    return productList\r\n  })\r\n\r\n  const changeCartItemCheck = (shopId, productId) => {\r\n    store.commit('changeCartItemChecked', { shopId, productId })\r\n  }\r\n\r\n  return { total, price, cartList, productList, changeItemInfo, changeCartItemCheck }\r\n}\r\n\r\nexport default {\r\n  name: 'Cart',\r\n  setup () {\r\n    const route = useRoute()\r\n    const shopId = route.params.id\r\n    const { total, price, productList, changeItemInfo, changeCartItemCheck } = useCartEffect(shopId)\r\n    return { total, price, shopId, productList, changeItemInfo, changeCartItemCheck }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables';\r\n@import '../../style/mixins';\r\n.cart {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n}\r\n.check {\r\n  display: flex;\r\n  height: .49rem;\r\n  border-top: .01rem solid $content-bgColor;\r\n  line-height: .49rem;\r\n  &__icon {\r\n    display: block;\r\n    position: relative;\r\n    width: .84rem;\r\n    &__img {\r\n      display: block;\r\n      width: .28rem;\r\n      height: .26rem;\r\n      margin: .12rem auto;\r\n    }\r\n    &__tag {\r\n      position: absolute;\r\n      left: .46rem;\r\n      top: .04rem;\r\n      padding: 0 .04rem;\r\n      min-width: .2rem;\r\n      height: .2rem;\r\n      line-height: .2rem;\r\n      background: $heightlight-fontColor;\r\n      border-radius: .1rem;\r\n      font-size: .12rem;\r\n      text-align: center;\r\n      color: $bgColor;\r\n      transform: scale(.5);\r\n      transform-origin: left center;\r\n    }\r\n  }\r\n  &__info {\r\n    flex: 1;\r\n    color: #333;\r\n    font-size: .12rem;\r\n    &__price {\r\n      color: $heightlight-fontColor;\r\n      font-size: .18rem;\r\n      margin-left: .05rem;\r\n    }\r\n  }\r\n  &__btn {\r\n    width: .98rem;\r\n    background: #4FB0F9;\r\n    text-align: center;\r\n    font-size: .14rem;\r\n    color: $bgColor;\r\n  }\r\n}\r\n.product {\r\n  overflow-y: scroll;\r\n  flex: 1;\r\n  background: #FFF;\r\n  &__header {\r\n    height: .52rem;\r\n    border-bottom: #F1F1F1;\r\n  }\r\n  &__item {\r\n    position: relative;\r\n    display: flex;\r\n    padding: .12rem 0;\r\n    margin: 0 .16rem;\r\n    border-bottom: .01rem solid $content-bgColor;\r\n    &__checked {\r\n      line-height: .5rem;\r\n      margin-right: .2rem;\r\n      color: #0091FF;\r\n      font-size: .2rem;\r\n    }\r\n    &__img {\r\n      width: .46rem;\r\n      height: .46rem;\r\n    }\r\n    &__detail {\r\n      margin-left: .16rem;\r\n      overflow: hidden;\r\n    }\r\n    &__title {\r\n      margin: 0;\r\n      line-height: .2rem;\r\n      font-size: .12rem;\r\n      color: $content-fontcolor;\r\n      @include ellipsis;\r\n    }\r\n    &__price {\r\n      margin: .06rem 0 0 0;\r\n      font-size: .14rem;\r\n      color: #ac8c8c;\r\n      line-height: .2rem;\r\n    }\r\n    &__yen {\r\n      font-size: .12rem;\r\n    }\r\n    &__origin {\r\n      font-size: .12rem;\r\n      color: $light-fontColor;\r\n      line-height: 20px;\r\n      margin-left: .06rem;\r\n      text-decoration: line-through;\r\n    }\r\n    .product__number {\r\n      position: absolute;\r\n      bottom: .12rem;\r\n      right: 0;\r\n      &__minus, &__plus {\r\n        display: inline-block;\r\n        height: .2rem;\r\n        line-height: .16rem;\r\n        width: .2rem;\r\n        border-radius: 50%;\r\n        font-size: .2rem;\r\n        text-align: center;\r\n      }\r\n      &__minus {\r\n        border: 0.01rem solid $medium-fontColor;\r\n        color: $medium-fontColor;\r\n        margin-right: .05rem;\r\n      }\r\n      &__plus {\r\n        background: $btn-bgColor;\r\n        color: $bgColor;\r\n        margin-left: .05rem;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}