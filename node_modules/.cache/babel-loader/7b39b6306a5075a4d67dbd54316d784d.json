,CAAC,oBAAD,EAAuB;AAC9CO,0BAAQ,EAAED,IAAI,CAACS,SAD+B;AAE9CP,0BAAQ,EAAEF,IAAI,CAACE;AAF+B,iBAAvB,CAFT;;AAAA;AAEVQ,sBAFU;;AAMhB,oBAAI,OAAM,SAAN,UAAM,WAAN,kBAAM,CAAEC,KAAR,MAAkB,CAAtB,EAAyB;AACvBC,8BAAY,CAACC,OAAb,GAAuB,IAAvB;AACAR,wBAAM,CAACS,IAAP,CAAY;AAAEjB,wBAAI,EAAE;AAAR,mBAAZ;AACF,iBAHA,MAGO;AACLM,2BAAS;AACTH,sBAAI,CAACG,SAAL,GAAiB,IAAjB;AACAH,sBAAI,CAACI,YAAL,GAAoB,MAApB;AACAG,4BAAU,CAAC,YAAM;AACfP,wBAAI,CAACG,SAAL,GAAiB,KAAjB;AACAH,wBAAI,CAACI,YAAL,GAAoB,EAApB;AACD,mBAHS,EAGP,IAHO,CAAV;AAIF;;AAjBgB;AAAA;;AAAA;AAAA;AAAA;AAmBhBW,qBAAK,CAAC,MAAD,CAAL;;AAnBgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAJ;;AAAA,sBAAVP,WAAU;AAAA;AAAA;AAAA,OAAhB;;AAsBA,QAAMQ,mBAAkB,GAAI,SAAtBA,mBAAsB,GAAM;AAChCX,YAAM,CAACS,IAAP,CAAY;AAAEjB,YAAI,EAAE;AAAR,OAAZ;AACF,KAFA;;AAGA,WAAO;AAAEW,iBAAW,EAAXA,WAAF;AAAeQ,yBAAmB,EAAnBA,mBAAf;AAAoCb,eAAS,EAATA,SAApC;AAA+CH,UAAG,EAAHA;AAA/C,KAAP;AACF;AA7Ca,CAAf","names":["useRouter","post","reactive","Toast","name","components","setup","data","username","password","showToast","toastMessage","router","message","setTimeout","handleLogin","usernamse","result","errno","localStorage","isLogin","push","alert","handleRegisterClick"],"sourceRoot":"","sources":["C:\\Users\\a\\Desktop\\Vue3.0\\京东到家项目\\京东到家3\\jingdong\\src\\views\\login\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n      <img class=\"wrapper__img\" src=\"http://www.dell-lee.com/imgs/vue3/user.png\">\r\n      <div class=\"wrapper__input\">\r\n          <input\r\n           class=\"wrapper__input__content\"\r\n           placeholder=\"请输入用户名\"\r\n           v-model=\"data.username\"\r\n          />\r\n      </div>\r\n      <div class=\"wrapper__input\">\r\n          <input\r\n           class=\"wrapper__input__content\"\r\n           placeholder=\"请输入密码\"\r\n           type=\"password\"\r\n           v-model=\"data.password\"\r\n          />\r\n      </div>\r\n      <div class=\"wrapper__login-button\" @click=\"handleLogin\">登录</div>\r\n      <div class=\"wrapper__login-link\" @click=\"handleRegisterClick\">立即注册</div>\r\n  </div>\r\n  <toast v-if=\"data.showToast\" :message=\"data.toastMessage\"/>\r\n</template>\r\n\r\n<script>\r\nimport { useRouter } from 'vue-router'\r\nimport { post } from '../../utils/request'\r\nimport { reactive } from '@vue/reactivity'\r\nimport Toast from '../../components/Toast'\r\n\r\nexport default {\r\n  name: 'Login',\r\n  components: { Toast },\r\n  setup () {\r\n    const data = reactive({\r\n      username: '',\r\n      password: '',\r\n      showToast: false,\r\n      toastMessage: ''\r\n    })\r\n    const router = useRouter()\r\n    const showToast = (message) => {\r\n      data.showToast = true\r\n      data.toastMessage = message\r\n      setTimeout(() => {\r\n        data.showToast = false\r\n        data.toastMessage = ''\r\n      }, 2000)\r\n    }\r\n    const handleLogin = async () => {\r\n      try {\r\n        const result = await post('111/api/user/login', {\r\n          username: data.usernamse,\r\n          password: data.password\r\n        })\r\n        if (result?.errno === 0) {\r\n          localStorage.isLogin = true\r\n          router.push({ name: 'Home' })\r\n        } else {\r\n          showToast()\r\n          data.showToast = true\r\n          data.toastMessage = '登录失败'\r\n          setTimeout(() => {\r\n            data.showToast = false\r\n            data.toastMessage = ''\r\n          }, 2000)\r\n        }\r\n      } catch (e) {\r\n        alert('请求失败')\r\n      }\r\n    }\r\n    const handleRegisterClick = () => {\r\n      router.push({ name: 'Register' })\r\n    }\r\n    return { handleLogin, handleRegisterClick, showToast, data }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/virables.scss';\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n  &__img {\r\n    display: block;\r\n    height: .66rem;\r\n    width: .66rem;\r\n    margin: 0 auto .4rem auto;\r\n  }\r\n  &__input {\r\n    height: .48rem;\r\n    margin: 0 .4rem .16rem .4rem;\r\n    background: #F9F9F9;\r\n    padding: 0 .16rem;\r\n    border: 1px solid rgba(0,0,0,0.10);\r\n    border-radius: .06rem;\r\n    border-radius: .06rem;\r\n    &__content {\r\n      width: 100%;\r\n      line-height: .48rem;\r\n      border: none;\r\n      background: none;\r\n      outline: none;\r\n      font-size: .16rem;\r\n      color: $content-notice-fontcolor;\r\n      &::placeholder {\r\n        color: $content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n  &__login-button {\r\n    margin: .32rem .4rem .16rem .4rem;\r\n    line-height: .48rem;\r\n    background: #0091FF;\r\n    box-shadow: 0 .04rem .08rem 0 rgba(0,145,255,0.32);\r\n    border-radius: .04rem;\r\n    border-radius: .04rem;\r\n    font-size: .16rem;\r\n    color: #FFFFFF;\r\n    text-align: center;\r\n  }\r\n  &__login-link {\r\n    font-size: 14px;\r\n    color: $content-notice-fontcolor;\r\n    text-align: center;\r\n  }\r\n}\r\n</style>\r\n"]}]}